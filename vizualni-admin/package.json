{
  "name": "vizualni-admin",
  "version": "1.0.0",
  "description": "Модерна администрациона библиотека за визуелизацију података са подршком за српски језик / Modern administrative library for data visualization with Serbian language support",
  "keywords": [
    "react",
    "typescript",
    "admin",
    "dashboard",
    "charts",
    "tables",
    "serbian",
    "localization",
    "data-visualization",
    "ui-components",
    "кирилица",
    "српски",
    "lingui",
    "i18n",
    "cyrillic",
    "latin"
  ],
  "homepage": "https://vizualni-admin.com",
  "bugs": {
    "url": "https://github.com/your-org/vizualni-admin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/vizualni-admin.git"
  },
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Vizualni Admin Team",
    "email": "team@vizualni-admin.com",
    "url": "https://vizualni-admin.com"
  },
  "maintainers": [
    {
      "name": "Петар Петровић",
      "email": "petar@vizualni-admin.com"
    },
    {
      "name": "Марина Јовановић",
      "email": "marina@vizualni-admin.com"
    }
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    },
    "./dist/styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    },
    "./locales": "./locales",
    "./fonts": "./fonts"
  },
  "sideEffects": [
    "*.css",
    "*.woff2",
    "*.woff"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@lingui/react": "^4.7.0",
    "@lingui/core": "^4.7.0",
    "@lingui/macro": "^4.7.0",
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "date-fns": "^2.30.0",
    "clsx": "^2.1.0",
    "react-hook-form": "^7.49.2",
    "@hookform/resolvers": "^3.3.4",
    "yup": "^1.4.0",
    "framer-motion": "^10.16.16",
    "react-intersection-observer": "^9.5.3",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.8",
    "lodash-es": "^4.17.21",
    "date-fns-tz": "^2.0.0",
    "csv-parse": "^5.5.6",
    "make-plural": "^7.3.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-use": "^17.4.0",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "axios": "^1.6.2",
    "react-router-dom": "^6.20.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-window": "^1.8.8",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.1.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/user-event": "^14.5.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@playwright/test": "^1.41.1",
    "rollup": "^4.9.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "postcss": "^8.4.32",
    "autoprefixer": "^10.4.16",
    "tailwindcss": "^3.4.0",
    "@storybook/react": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/addon-essentials": "^7.6.10",
    "storybook": "^7.6.10",
    "vite-plugin-dts": "^3.7.2",
    "typedoc": "^0.25.7",
    "conventional-changelog": "^4.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^22.0.12",
    "@lingui/cli": "^4.7.0",
    "@lingui/loader": "^4.7.0",
    "vite-plugin-pwa": "^0.17.4",
    "workbox-window": "^7.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && rollup -c",
    "build:lib": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "build:docs": "typedoc src/index.ts",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:ci": "jest --coverage --watchAll=false",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "localize": "npm run extract && npm run compile",
    "fonts:subset": "node scripts/subset-fonts.js",
    "demo:data": "node scripts/generate-demo-data.js",
    "pre-commit": "npm run lint && npm run typecheck && npm run test && npm run localize && npm run build",
    "prepare": "echo 'Skipping prepare during install'",
    "prepublishOnly": "npm run test && npm run build && npm run localize",
    "release": "semantic-release",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "analyze": "npm run build && npx analyze-dist"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setup.ts"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.tsx",
      "!src/test/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/your-org"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "overrides": {
    "@mdx-js/react": {
      "react": "^18.0.0"
    },
    "eslint": {
      "strip-ansi": "^6.0.1"
    }
  }
}